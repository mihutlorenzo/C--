      int zzCh;
      int zzCharCount;
      for (zzCurrentPosL = zzStartRead  ;
           zzCurrentPosL < zzMarkedPosL ;
           zzCurrentPosL += zzCharCount ) {
        zzCh = Character.codePointAt(zzBufferL, zzCurrentPosL, zzMarkedPosL);
        zzCharCount = Character.charCount(zzCh);
        switch (zzCh) {
        case '\u000B':  // fall through
        case '\u000C':  // fall through
        case '\u0085':  // fall through
        case '\u2028':  // fall through
        case '\u2029':
          yyline++;
          yycolumn = 0;
          zzR = false;
          break;
        case '\r':
          yyline++;
          yycolumn = 0;
          zzR = true;
          break;
        case '\n':
          if (zzR)
            zzR = false;
          else {
            yyline++;
            yycolumn = 0;
          }
          break;
        default:
          zzR = false;
          yycolumn += zzCharCount;
        }
      }

      if (zzR) {
        // peek one character ahead if it is \n (if we have counted one line too much)
        boolean zzPeek;
        if (zzMarkedPosL < zzEndReadL)
          zzPeek = zzBufferL[zzMarkedPosL] == '\n';
        else if (zzAtEOF)
          zzPeek = false;
        else {
          boolean eof = zzRefill();
          zzEndReadL = zzEndRead;
          zzMarkedPosL = zzMarkedPos;
          zzBufferL = zzBuffer;
          if (eof) 
            zzPeek = false;
          else 
            zzPeek = zzBufferL[zzMarkedPosL] == '\n';
        }
        if (zzPeek) yyline--;
      }
      zzAction = -1;

      zzCurrentPosL = zzCurrentPos = zzStartRead = zzMarkedPosL;
  
      zzState = ZZ_LEXSTATE[zzLexicalState];

      // set up zzAction for empty match case:
      int zzAttributes = zzAttrL[zzState];
      if ( (zzAttributes & 1) == 1 ) {
        zzAction = zzState;
      }


      zzForAction: {
        while (true) {
    
          if (zzCurrentPosL < zzEndReadL) {
            zzInput = Character.codePointAt(zzBufferL, zzCurrentPosL, zzEndReadL);
            zzCurrentPosL += Character.charCount(zzInput);
          }
          else if (zzAtEOF) {
            zzInput = YYEOF;
            break zzForAction;
          }
          else {
            // store back cached positions
            zzCurrentPos  = zzCurrentPosL;
            zzMarkedPos   = zzMarkedPosL;
            boolean eof = zzRefill();
            // get translated positions and possibly new buffer
            zzCurrentPosL  = zzCurrentPos;
            zzMarkedPosL   = zzMarkedPos;
            zzBufferL      = zzBuffer;
            zzEndReadL     = zzEndRead;
            if (eof) {
              zzInput = YYEOF;
              break zzForAction;
            }
            else {
              zzInput = Character.codePointAt(zzBufferL, zzCurrentPosL, zzEndReadL);
              zzCurrentPosL += Character.charCount(zzInput);
            }
          }
          int zzNext = zzTransL[ zzRowMapL[zzState] + zzCMapL[zzInput] ];
          if (zzNext == -1) break zzForAction;
          zzState = zzNext;

          zzAttributes = zzAttrL[zzState];
          if ( (zzAttributes & 1) == 1 ) {
            zzAction = zzState;
            zzMarkedPosL = zzCurrentPosL;
            if ( (zzAttributes & 8) == 8 ) break zzForAction;
          }

        }
      }

      // store back cached position
      zzMarkedPos = zzMarkedPosL;

      if (zzInput == YYEOF && zzStartRead == zzCurrentPos) {
        zzAtEOF = true;
            zzDoEOF();
            switch (zzLexicalState) {
            case YYINITIAL: {
              return symbol(EOF);
            }  // fall though
            case 103: break;
            default:
          { return new java_cup.runtime.Symbol(sym.EOF); }
        }
      }
      else {
        switch (zzAction < 0 ? zzAction : ZZ_ACTION[zzAction]) {
          case 1: 
            { /* ignore */
            } 
            // fall through
          case 57: break;
          case 2: 
            { throw new RuntimeException("Illegal character \"" + yytext() +
                                                              "\" at line " + yyline + ", column " + yycolumn);
            } 
            // fall through
          case 58: break;
          case 3: 
            { return symbol(O_DIV);
            } 
            // fall through
          case 59: break;
          case 4: 
            { return symbol(O_MULT);
            } 
            // fall through
          case 60: break;
          case 5: 
            { return symbol(IDENTIFIER, yytext());
            } 
            // fall through
          case 61: break;
          case 6: 
            { return symbol(INTEGER_LITERAL, new Integer(Integer.parseInt(yytext())));
            } 
            // fall through
          case 62: break;
          case 7: 
            { string.setLength(0); yybegin(STRING);
            } 
            // fall through
          case 63: break;
          case 8: 
            { return symbol(O_DOT);
            } 
            // fall through
          case 64: break;
          case 9: 
            { return symbol(O_SUB);
            } 
            // fall through
          case 65: break;
          case 10: 
            { return symbol(O_ADD);
            } 
            // fall through
          case 66: break;
          case 11: 
            { return symbol(O_NOT);
            } 
            // fall through
          case 67: break;
          case 12: 
            { return symbol(O_EQL);
            } 
            // fall through
          case 68: break;
          case 13: 
            { return symbol(O_LT);
            } 
            // fall through
          case 69: break;
          case 14: 
            { return symbol(O_GT);
            } 
            // fall through
          case 70: break;
          case 15: 
            { return symbol(O_SEMICOLON);
            } 
            // fall through
          case 71: break;
          case 16: 
            { return symbol(O_COMA);
            } 
            // fall through
          case 72: break;
          case 17: 
            { return symbol(K_LPAREN);
            } 
            // fall through
          case 73: break;
          case 18: 
            { return symbol(K_RPAREN);
            } 
            // fall through
          case 74: break;
          case 19: 
            { return symbol(K_LSQBKT);
            } 
            // fall through
          case 75: break;
          case 20: 
            { return symbol(K_RSQBKT);
            } 
            // fall through
          case 76: break;
          case 21: 
            { return symbol(K_LBRKT);
            } 
            // fall through
          case 77: break;
          case 22: 
            { return symbol(K_RBRKT);
            } 
            // fall through
          case 78: break;
          case 23: 
            { string.append( yytext() );
            } 
            // fall through
          case 79: break;
          case 24: 
            { throw new RuntimeException("Unterminated string at end of line");
            } 
            // fall through
          case 80: break;
          case 25: 
            { yybegin(YYINITIAL);return symbol(STRING_LITERAL,string.toString());
            } 
            // fall through
          case 81: break;
          case 26: 
            { return symbol(O_NOTEQL);
            } 
            // fall through
          case 82: break;
          case 27: 
            { return symbol(O_AND);
            } 
            // fall through
          case 83: break;
          case 28: 
            { return symbol(O_OR);
            } 
            // fall through
          case 84: break;
          case 29: 
            { return symbol(O_DEQL);
            } 
            // fall through
          case 85: break;
          case 30: 
            { return symbol(O_LE);
            } 
            // fall through
          case 86: break;
          case 31: 
            { return symbol(O_LSHIFT);
            } 
            // fall through
          case 87: break;
          case 32: 
            { return symbol(O_GE);
            } 
            // fall through
          case 88: break;
          case 33: 
            { return symbol(O_RSHIFT);
            } 
            // fall through
          case 89: break;
          case 34: 
            { return symbol(K_IF);
            } 
            // fall through
          case 90: break;
          case 35: 
            { throw new RuntimeException("Illegal escape sequence \"" + yytext() + "\"");
            } 
            // fall through
          case 91: break;
          case 36: 
            { string.append( '\u000b' );
            } 
            // fall through
          case 92: break;
          case 37: 
            { string.append( '\0' );
            } 
            // fall through
          case 93: break;
          case 38: 
            { string.append( '\"' );
            } 
            // fall through
          case 94: break;
          case 39: 
            { string.append( '\\' );
            } 
            // fall through
          case 95: break;
          case 40: 
            { string.append( '\n' );
            } 
            // fall through
          case 96: break;
          case 41: 
            { string.append( '\t' );
            } 
            // fall through
          case 97: break;
          case 42: 
            { string.append( '\b' );
            } 
            // fall through
          case 98: break;
          case 43: 
            { string.append( '\r' );
            } 
            // fall through
          case 99: break;
          case 44: 
            { string.append( '\f' );
            } 
            // fall through
          case 100: break;
          case 45: 
            { string.append( '\u0007' );
            } 
            // fall through
          case 101: break;
          case 46: 
            { string.append( '\'' );
            } 
            // fall through
          case 102: break;
          case 47: 
            { return symbol(K_INT);
            } 
            // fall through
          case 103: break;
          case 48: 
            { return symbol (K_CIN);
            } 
            // fall through
          case 104: break;
          case 49: 
            { return symbol(K_VOID);
            } 
            // fall through
          case 105: break;
          case 50: 
            { return symbol(K_TRUE);
            } 
            // fall through
          case 106: break;
          case 51: 
            { return symbol(K_BOOL);
            } 
            // fall through
          case 107: break;
          case 52: 
            { return symbol(K_ELSE);
            } 
            // fall through
          case 108: break;
          case 53: 
            { return symbol (K_COUT);
            } 
            // fall through
          case 109: break;
          case 54: 
            { return symbol(K_FALSE);
            } 
            // fall through
          case 110: break;
          case 55: 
            { return symbol(K_WHILE);
            } 
            // fall through
          case 111: break;
          case 56: 
            { return symbol(K_RETURN);
            } 
            // fall through
          case 112: break;
          default:
            zzScanError(ZZ_NO_MATCH);
        }
      }
    }
  }


}
